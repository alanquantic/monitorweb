{
  "functions": {
    "api/monitor.js": {
      "maxDuration": 300
    }
  },
  "crons": [
    {
      "path": "/api/monitor",
      "schedule": "0 */8 * * *"
    }
  ],
  "builds": [
    {
      "src": "api/monitor.js",
      "use": "@vercel/node"
    }
  ],
  "env": {
    "PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD": "1"
  }
} 